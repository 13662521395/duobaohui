<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/no_date_layout_icon"
        android:layout_width="@dimen/s_55dp"
        android:layout_height="@dimen/s_55dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/s_150dp" />

    <com.shinc.duobaohui.customview.imp.CustomTextView
        android:id="@+id/no_date_layout_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/no_date_layout_icon"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/s_15dp"
        android:textColor="@color/c_afafaf"
        android:textSize="@dimen/s_14sp" />


    <com.shinc.duobaohui.customview.imp.CustomTextView
        android:id="@+id/goto_duobao"
        style="@style/red_btn_style"
        android:layout_width="@dimen/s_120dp"
        android:layout_height="@dimen/s_35dp"
        android:layout_below="@id/no_date_layout_tv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/s_15dp"
        android:gravity="center"
        android:textColor="@android:color/white"
        android:visibility="gone" />
</RelativeLayout>